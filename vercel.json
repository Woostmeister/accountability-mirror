{
  "version": 2,
  "builds": [
    {
      "src": "accountability-mirror-single.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/claude.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/claude",
      "dest": "/api/claude.js"
    },
    {
      "src": "/(.*)",
      "dest": "/accountability-mirror-single.html"
    }
  ]
}
